`timescale 1ps / 1ps

module sqrlog_tb (	);

	parameter HALF_NIOS_CYCLE = 5000;
	parameter HALF_FFT_CYCLE  = 12500;

		wire  [31:0] data;
		reg       nios_ready;
		wire     nios_valid, nios_sop, nios_eop;
		wire   [1:0]  nios_empty;
		reg         nios_clk, fft_clk;   
		reg         reset;
        reg   [31:0] fft_re;
		  reg   [31:0] fft_im;
		  reg   fft_valid, fft_sop, fft_eop,fft_go;
		  wire fft_ready;
		  
		  reg[2:0] nios_stage;
		  reg[7:0] count1;
		  reg[2:0] fft_stage;
		  reg[7:0] count2;
initial
  begin
    
//    $dumpvars;
    #0 nios_clk = 1'b0;
    #0 fft_clk = 1'b0;
    #0 reset = 1'b1;
    #(5*HALF_FFT_CYCLE) reset = 1'b0;
    
  end

	// тактовые
always  begin
    #HALF_NIOS_CYCLE    nios_clk = 1;
    #HALF_NIOS_CYCLE    nios_clk = 0;
end
always  begin
    #HALF_FFT_CYCLE fft_clk = 1;
    #HALF_FFT_CYCLE fft_clk = 0;
end

